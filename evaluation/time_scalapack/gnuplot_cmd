set style histogram rowstacked
set boxwidth 0.5 relative
set style fill solid border lc rgb "black"
set title "N = 10000"
set xlabel "#procs"
set ylabel "elapse time [sec]"
#set xtics rotate by -90
plot "data" using 2:xtic(1)  with histogram title columnhead,\
     "data" using 3          with histogram title columnhead,\
     "data" using 4          with histogram title columnhead
replot